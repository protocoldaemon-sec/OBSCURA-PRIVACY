{
  "name": "@obscura/backend",
  "version": "0.1.0",
  "description": "Off-chain services: SIP privacy, PQ auth, aggregator/executor",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./sip": {
      "types": "./dist/sip/index.d.ts",
      "import": "./dist/sip/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.js"
    },
    "./executor": {
      "types": "./dist/executor/index.d.ts",
      "import": "./dist/executor/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@arcium-hq/client": "^0.6.1",
    "@arcium-hq/reader": "^0.6.3",
    "@hono/node-server": "^1.8.0",
    "@lightprotocol/compressed-token": "^0.22.0",
    "@lightprotocol/stateless.js": "^0.22.0",
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^1.3.3",
    "@obscura/crypto": "workspace:*",
    "@sip-protocol/sdk": "^0.7.2",
    "@sip-protocol/types": "^0.2.1",
    "@solana/web3.js": "^1.98.4",
    "bn.js": "^5.2.2",
    "bs58": "^6.0.0",
    "circomlibjs": "^0.1.7",
    "dotenv": "^17.2.3",
    "hono": "^4.0.0",
    "snarkjs": "^0.7.6",
    "viem": "^2.44.2"
  },
  "devDependencies": {
    "@types/bn.js": "^5.2.0",
    "@types/node": "^20.10.0",
    "fast-check": "^3.22.0",
    "tsx": "^4.7.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist",
    "src"
  ]
}
